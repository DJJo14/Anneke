[GENERAL]
# With out ".", "{", "}", "/0" and "@"
#Adress anneke server
host='localhost'
#port anneke server
port='5200'

[dvbworker]
[[dvblast1]]
#the base command for dvblast
basecommand='dvblast -a 0 -l -g dvblast1'
ctlcommand='dvblastctl'
#freq the worker can work with
freq=2,5
#worker priority
priority=10

[[dvblast2]]
#the base command for dvblast
basecommand='dvblast -a 1 -l -g dvblast2'
ctlcommand='dvblastctl'
#freq the worker can work with
freq=1,2,5
#worker priority
priority=10


[decriptworker]
[[tsdecript1]]
#ip Adress worker
address=127.0.0.1
#the base command for dvblast
basecommand='tsdecrypt -F tsdecipt1.dvb -U <user> -P <pass>'
#worker priority
priority=10

[[tsdecript2]]
#ip Adress worker
address=127.0.0.1
#the base command for dvblast
basecommand='tsdecrypt -F tsdecipt1.dvb -U <user> -P <pass>'
#worker priority
priority=10

